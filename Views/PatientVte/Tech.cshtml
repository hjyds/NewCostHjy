    <input type="text" placeholder="Pvid" id="pvid" value="" autocomplete="off" />
    <input type="text" placeholder="Prid" id="prid" value="" autocomplete="off" />
    <input type="text" placeholder="Sitesn" id="sitesn" value="" autocomplete="off" />
</div> 

<div id="inputarea">

</div>
<div class="eleAdviceThirdPage">
    <iframe class="hjy_iframe">
    </iframe>
</div>

<script>
    var _mvcData = @Html.Raw(JsonConvert.SerializeObject(ViewBag.BaseData));
    //alert(JSON.stringify(_mvcData));
    setTimeout(function () {
        $("#parin").html(JSON.stringify(_mvcData));
    },100);
</script>

<script src="~/module/patientvte/tech.js" asp-append-version="true"></script>


