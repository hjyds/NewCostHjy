@using Newtonsoft.Json

<link rel="stylesheet" href="~/module/patientvte/tech.css" />

<div class="sys_tag" style="display: none;">
    测试医技站主页界面
</div>
<div style="display: none;"> 入参信息：</div>
<div id="parin" style="display: none;"></div>
<div>
    <input type="text" placeholder="IP+PORT" id="svr_info" value="" autocomplete="off" />
    <input type="text" placeholder="帐号" id="user" value="" autocomplete="off" />
    <input type="text" placeholder="密码" id="pwd" value="" autocomplete="off" />
    <input type="text" placeholder="Token" id="token" value="" autocomplete="off" /> 
    <input type="text" placeholder="Type" id="type" value="" autocomplete="off" />
